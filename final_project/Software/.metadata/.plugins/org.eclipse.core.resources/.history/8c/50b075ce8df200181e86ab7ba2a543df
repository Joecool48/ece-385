#ifndef INCLUDE_SPRITE_H_
#define INCLUDE_SPRITE_H_

class Image {
public:
	unsigned x;
	unsigned y;
	unsigned width;
	unsigned height;
	unsigned start_address;
};
class Sprite : public Image {
	unsigned x;
	unsigned y;
};


#endif /* INCLUDE_SPRITE_H_ */
