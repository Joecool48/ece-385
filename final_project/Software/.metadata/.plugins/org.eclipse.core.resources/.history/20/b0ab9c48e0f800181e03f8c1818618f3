#include "enemies.h"


void Gumba::animatorSetup() {
	state_mode_to_frames_map[WALKING][NORMAL_MODE] = Sprite()
}

void Turtle::animatorSetup() {
	state_mode_to_frames_map[WALKING][NORMAL_MODE].push_back(Sprite(x, y, 16, 32, 759584));
	state_mode_to_frames_map[WALKING][NORMAL_MODE].push_back(Sprite(x,y,16,32,760096));
	state_mode_to_frames_map[SHELL][NORMAL_MODE].push_back(Sprite(x,y,16,32,760608));
}

