#include "item.h"

Item::Item (uint16_t start_x, uint16_t start_y, uint16_t start_mode, uint16_t start_state, float start_frame, Rect_Collider rect) : Sprite_Animator (start_x, start_y, start_mode, start_state, start_frame), collider(rect){
Fireflower::Fireflower(uint16_t start_x, uint16_t start_y, uint16_t start_mode, uint16_t start_state, float start_frame, Rect_Collider rect) : Item (start_x, start_y, start_mode, start_state, start_frame, rect) {

}
}



